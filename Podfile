xcodeproj 'CXHExperienceStores.xcodeproj'
source 'https://github.com/CocoaPods/Specs.git'
platform :ios, ’8.0’
use_frameworks!
target ‘CXHExperienceStores’ do
	pod 'Alamofire'       #网络请求(swift)
	pod 'ObjectMapper'     #一键json模型互转
	pod 'IQKeyboardManagerSwift'  #管理键盘
	pod 'Siren' #版本更新
	pod 'SVProgressHUD'    #菊花动画
	pod 'XCGLogger' #日志管理类库
	pod 'RealReachability' #实时监测网络
    pod 'Result'
    pod 'Onboard'#引导页
	pod 'Moya' #对Alamofire的封装
    pod 'JPush' #极光推送
    pod 'SwiftyJSON' #JSON解析
#    pod 'PageMenu' #分页
    pod 'MJRefresh' #下拉刷新
    pod 'BaiduMobStat' #百度统计
    pod 'Kingfisher' #图片缓存
end

